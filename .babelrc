{
  "presets": ["env"],
  "plugins": [
    "transform-class-properties",
    "transform-react-jsx",
    "transform-object-rest-spread",
    ["module-resolver", {
      "root": ["./src"]
    }],
    ["import-rename", {"^(.*)\\.jsx$": "$1"}]
  ],
  "ignore": ["**/push-sw.js",
    "./src/push-sw.js"]
}
